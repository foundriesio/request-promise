{
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "es6": true,
        "mocha": true,
        "node": true
    },
    "rules": {
        "require-jsdoc": "off",
        "no-unused-vars": "error",
        "no-undef": "error"
    }
}
